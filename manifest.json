{
  "manifest_version": 3,
  "name": "Flow Runner",
  "version": "1.0.0",
  "description": "快速触发 RunFlow API",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["https://*/*"]
}
